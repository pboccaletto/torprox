{
    "manifest_version": 2,
    "name": "Torprox",
    "version": "1.0",
    "description": "Set SOCKS5 proxy to 127.0.0.1:9050 with one click.",
    "permissions": ["proxy", "activeTab"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Set SOCKS5 Proxy"
    },
    "icons": {
        "48": "icon.png"
    }
}
